<h1>Add New</h1>

<%= form_for(@ad) do |f| %>

<p><b>name</b><br><%= f.text_field :name %></p>

<p><b>price</b><br><%= f.text_area :price %></p>

<p><b>seller</b><br><%= f.text_field :seller_id %></p>

<p><b>email</b><br><%= f.text_field :email %></p>

<p><b>img</b><br><%= f.text_field :img_url %></p>

<%= f.submit "Create" %>

<% end %>